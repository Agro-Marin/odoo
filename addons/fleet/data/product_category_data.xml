<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="product_category_vehicle_services" model="product.category">
        <field name="parent_id" eval="ref('product.product_category_goods', raise_if_not_found=False)"/>
        <field name="name">Vehicles</field>
    </record>

    <record id="product_category_vehicle_contracts" model="product.category">
        <field name="parent_id" eval="ref('product.product_category_services', raise_if_not_found=False)"/>
        <field name="name">Fleet contracts</field>
    </record>

    <record id="product_category_vehicle_maintenance" model="product.category">
        <field name="parent_id" eval="ref('product.product_category_expenses', raise_if_not_found=False)"/>
        <field name="name">Maintenance &amp; Repair</field>
    </record>

    <record id="product_category_vehicle_others" model="product.category">
        <field name="parent_id" eval="ref('product.product_category_services', raise_if_not_found=False)"/>
        <field name="name">Fleet - Others</field>
    </record>

</odoo>